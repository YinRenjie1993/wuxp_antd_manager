{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    [
      "stage-2"
    ]
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    [
      "import",
      [
        {
          "libraryName": "antd",
          "libraryDirectory": "es"
        }
      ]
    ]
  ]
}
